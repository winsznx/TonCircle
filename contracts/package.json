{
    "name": "TonCircle",
    "version": "0.0.1",
    "scripts": {
        "bp": "blueprint",
        "start": "blueprint run",
        "build": "blueprint build",
        "test": "jest --verbose",
        "test:runner": "ts-node scripts/testRunner.ts",
        "test:factory": "ts-node scripts/testRunner.ts --suite GroupVaultFactory",
        "test:group": "ts-node scripts/testRunner.ts --suite GroupVault",
        "test:member": "ts-node scripts/testRunner.ts --suite Member",
        "test:integration": "ts-node scripts/testRunner.ts --suite Integration",
        "test:edge": "ts-node scripts/testRunner.ts --suite EdgeCases",
        "test:coverage": "ts-node scripts/testRunner.ts --coverage",
        "test:all": "ts-node scripts/testRunner.ts",
        "release": "blueprint pack && npm publish --access public"
    },
    "dependencies": {
        "@ton/core": "~0",
        "dotenv": "^16.6.1"
    },
    "devDependencies": {
        "@tact-lang/compiler": ">=1.6.13 <2.0.0",
        "@ton-community/func-js": ">=0.10.0",
        "@ton/blueprint": ">=0.40.0",
        "@ton/crypto": "^3.3.0",
        "@ton/sandbox": ">=0.37.0",
        "@ton/test-utils": ">=0.11.0",
        "@ton/tolk-js": ">=1.0.0",
        "@ton/ton": ">=15.3.1 <16.0.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^22.17.2",
        "jest": "^30.0.5",
        "prettier": "^3.6.2",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2"
    }
}
